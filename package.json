{
  "name": "ce-dev",
  "description": "Local Stack wrapper tool",
  "version": "1.2.4",
  "author": " @pm98zz-c",
  "bin": {
    "ce-dev": "./bin/run"
  },
  "bugs": "https://github.com/codeenigma/ce-dev/issues",
  "dependencies": {
    "@oclif/core": "^3.10.8",
    "@oclif/plugin-autocomplete": "^3.0.1",
    "@oclif/plugin-help": "^6.0.6",
    "inquirer": "^9.2.12",
    "inquirer-fuzzy-path": "^2.3.0",
    "js-yaml": "^4.1.0",
    "tslib": "^2.6.2"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.7",
    "@types/node": "^20.9.0",
    "@typescript-eslint/parser": "^6.10.0",
    "eslint": "^8.53.0",
    "eslint-config-oclif": "^5.0.0",
    "eslint-config-oclif-typescript": "^3.0.15",
    "eslint-plugin-jsdoc": "^46.9.0",
    "globby": "^14.0.0",
    "prettier": "^3.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "main": "lib/index.js",
  "oclif": {
    "additionalHelpFlags": ["-h"],
    "additionalVersionFlags": ["-v"],
    "default": ".",
    "commands": "./lib/commands",
    "bin": "ce-dev",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-autocomplete"
    ]
  },
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest",
    "readme": "mv README.md README.save && cp docs/Commands.md README.md && oclif-dev readme && mv README.md docs/Commands.md && mv README.save README.md",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
